modules = ["nodejs-20", "web", "python-3.12"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "web"

[[workflows.workflow]]
name = "web"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server.js"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 38807
externalPort = 3001

[[ports]]
localPort = 40169
externalPort = 3000

[[ports]]
localPort = 43105
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["python", "server.py"]
