modules = ["nodejs-20", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "web"

[[workflows.workflow]]
name = "web"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server.js"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33835
externalPort = 3003

[[ports]]
localPort = 35687
externalPort = 3002

[[ports]]
localPort = 38807
externalPort = 3001

[[ports]]
localPort = 40169
externalPort = 3000

[[ports]]
localPort = 43517
externalPort = 5000

[[ports]]
localPort = 43875
externalPort = 5173

[[ports]]
localPort = 45405
externalPort = 4200

[deployment]
deploymentTarget = "autoscale"
run = ["python", "server.py"]
